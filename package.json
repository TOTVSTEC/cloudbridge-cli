{
  "name": "cloudbridge",
  "version": "0.3.11-SNAPSHOT",
  "preferGlobal": true,
  "description": "A tool for creating and developing CloudBridge mobile apps.",
  "homepage": "http://www.totvs.com/",
  "bin": {
    "cloudbridge": "index.js",
    "cb": "index.js"
  },
  "scripts": {
    "release": "totvstec release --npm-publish",
    "release:minor": "totvstec release minor --npm-publish",
    "release:major": "totvstec release major --npm-publish",
    "test": "npm run test-local && npm run report",
    "report": "node ./spec/report.js",
    "coverage-local": "istanbul cover node_modules/jasmine/bin/jasmine.js",
    "test-local": "jasmine",
    "test-travis": "istanbul cover node_modules/jasmine/bin/jasmine.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "cloudbridge",
    "mobile",
    "app",
    "hybrid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/totvstec/cloudbridge-cli.git"
  },
  "contributors": [
    {
      "name": "Rogerio Ribeiro da Cruz",
      "email": "rogerio.ribeiro@totvs.com.br"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "archiver": "^1.3.0",
    "bower": "^1.8.0",
    "colors": "^1.1.2",
    "ejs": "^2.5.6",
    "inquirer": "^3.0.6",
    "optimist": "^0.6.1",
    "progress": "^1.1.8",
    "q": "^1.4.1",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.7",
    "totvs-platform-helper": "^1.1.4",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "unzip": "^0.1.11",
    "username": "^2.3.0",
    "winston": "^2.3.1",
    "wiredep": "^4.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.3",
    "jscs": "^3.0.7",
    "plato": "^1.7.0",
    "totvstec-tools": "^1.0.6"
  }
}
